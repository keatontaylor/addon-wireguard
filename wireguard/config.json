{
  "name": "WireGuard",
  "version": "0.0.20181222",
  "slug": "wireguard",
  "description": "WireGuard VPN",
  "url": "https://github.com/whiskerz007/addon-wireguard/wireguard",
  "startup": "application",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "i386"
  ],
  "boot": "auto",
  "hassio_api": true,
  "hassio_role": "default",
  "homeassistant_api": true,
  "host_network": true,
  "map": [ ],
  "devices": [
    "/dev/net/tun:/dev/net/tun:rwm"
  ],
  "privileged": [ "NET_ADMIN" ],
  "options": {
    "log_level": "info",
    "gateway": false,
    "port": 51820,
    "addresses": [
      "192.168.99.1/24"
    ],
    "dns": [
      "8.8.8.8"
    ],
    "private_key": "",
    "peers": [
      {
        "public_key": "",
        "preshared_key": "",
        "endpoint": "",
        "allowed_ips": [
            "192.168.99.2/32"
        ],
        "keepalive": 25
      }
    ]
  },
  "schema": {
    "log_level": "match(^(trace|debug|info|notice|warning|error|fatal)$)",
    "gateway": "bool",
    "port": "int",
    "addresses": [ "str" ],
    "dns": [ "str" ],
    "private_key": "str",
    "peers": [
      {
        "public_key": "str",
        "preshared_key": "str",
        "endpoint": "str",
        "allowed_ips": [ "str" ],
        "keepalive": "int"
      }
    ]
  },
  "environment": {
    "LOG_FORMAT": "{LEVEL}: {MESSAGE}"
  }
}
